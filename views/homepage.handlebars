<div class="posts">
  {{#each posts}}
  <article class="post-container">
    <div class="post-header">
      <h2><a class="post-title" href="/post/{{id}}">{{title}}</a></h2>
      <div class="post-time">Posted by {{user.username}} on {{format_date createdAt}}</div>
    </div>
    <div class="post-sep-bar"></div>
    <div class="post-body">
      {{body}}
    </div>
  </article>
  {{/each}}
</div>
